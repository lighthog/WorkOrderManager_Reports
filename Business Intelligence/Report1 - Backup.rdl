      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>-- This report identifies Service Techs whose vehicles are not returning GeoTab information
SELECT DISTINCT EmployeeFullName, E.Email, E.FirstName, V.VehicleNumber, V.GeotabSerialNum
	FROM Vehicle V 
	INNER JOIN VehicleInspection VI ON VI.EmployeeID = V.EmployeeID AND Date &gt;= DateAdd(d, -1, GetDate()) 
	INNER JOIN Employee E ON E.EmployeeID = V.EmployeeID 
	WHERE V.GeoTabID IS NOT NULL  
                     AND E.EmployeeID = @EmployeeID
	  AND E.Active = 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@EmployeeID">
            <Value>=Parameters!EmployeeID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="EmployeeID">
      <DataType>String</DataType>
      <Prompt>Employee ID</Prompt>
