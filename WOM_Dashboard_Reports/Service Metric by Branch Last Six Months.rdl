  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@rpBranch">
            <Value>=Parameters!rpBranch.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT	P.Description, P.EndDate, SUM(I.MerchAmt) AS Sales, SUM(I.CostAmt) AS Cost, 
                   (SUM(I.MerchAmt) - SUM(I.CostAmt)) / dbo.GREATEST(1, SUM(I.MerchAmt)) AS GM_Percent, 
                   (SUM(I.MerchAmt) - SUM(I.CostAmt)) AS GM_Margin
         FROM TSsoPastInvcHdr AS I 
         INNER JOIN ProfitabilityPeriod AS P ON I.InvcDate &gt;= P.BeginDate AND I.InvcDate &lt;= P.EndDate 
         LEFT OUTER JOIN (
                         SELECT  SUM(SOMerchAmt) AS OpenSales, SUM(SOCostAmt) AS OpenCost, Whse, COUNT(*) AS OpenOrderCount
                                FROM TSsoDocHdr AS I
                                WHERE        (Dept = 'SVC') AND (DocType = 2)
                                 GROUP BY Whse, Dept
	       ) AS A ON A.Whse = I.Whse
           WHERE       (I.Dept = 'SVC') AND I.Whse LIKE @rpBranch
               AND  (P.ProfitabilityPeriodID IN (
	            SELECT ProfitabilityPeriodID 
                                       FROM ProfitabilityPeriod 
                                       WHERE EndDate BETWEEN DateAdd(d, -61, GetDate()) AND DateAdd(d, 30, GetDate()))
	       )
           GROUP BY P.Description, P.EndDate, P.MonthYear
           ORDER BY P.EndDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EndDate">
          <DataField>EndDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Sales">
          <DataField>Sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Cost">
          <DataField>Cost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="GM_Percent">
          <DataField>GM_Percent</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="GM_Margin">
          <DataField>GM_Margin</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
