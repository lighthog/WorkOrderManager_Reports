  <DataSources>
    <DataSource Name="AceWOMDataSource">
      <DataSourceReference>AceWOMDataSource</DataSourceReference>
      <rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType>
      <rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">0dd9b628-3a41-4090-9a0f-7f5e69b90026</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EmployeeID">
            <Value>=Parameters!EmployeeID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT WorkOrderTaskId, WONum, Description, DueDate, EMP.EmployeeID, EMP.FirstName, EMP.LastName, EMP.Email
	FROM WorkOrderTask WOT
	INNER JOIN Employee EMP ON EMP.EmployeeID = WOT.AssignedTo
	WHERE DueDate &lt; GetDate()
      AND Complete = 0
      AND Email IS NOT NULL
      AND EmployeeID = @EmployeeID
      ORDER BY Email</CommandText>
        <rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="WorkOrderTaskId">
          <DataField>WorkOrderTaskId</DataField>
          <rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName>
        </Field>
        <Field Name="WONum">
          <DataField>WONum</DataField>
          <rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName>
        </Field>
        <Field Name="DueDate">
          <DataField>DueDate</DataField>
          <rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.DateTime</rd:TypeName>
        </Field>
        <Field Name="EmployeeID">
