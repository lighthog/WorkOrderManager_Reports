  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BranchId">
            <Value>=Parameters!BranchId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PayPeriodId">
            <Value>=Parameters!PayPeriodId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
 SELECT  E.EmployeeFullName, TE.Hours, TE.TimeType
FROM TimeException TE
INNER JOIN Employee E ON TE.EmployeeID = E.EmployeeID 
WHERE  TE.PayPeriodID = @PayPeriodId AND E.BranchId = @BranchId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="EmployeeFullName">
          <DataField>EmployeeFullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Hours">
          <DataField>Hours</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TimeType">
          <DataField>TimeType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSetPayPeriod">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>
SELECT PayPeriodId, CAST(StartDate AS VARCHAR(12)) + ' - ' +  CAST(EndDate AS VARCHAR(12)) 
    FROM PayPeriod WHERE StartDate &gt; DateAdd(d,-60,GetDate()) ORDER BY StartDate</CommandText>
      </Query>
      <Fields>
        <Field Name="PayPeriodId">
          <DataField>PayPeriodId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ID">
          <DataField />
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSetEmployees">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>
 SELECT EmployeeID, EmployeeFullName FROM Employee WHERE PayTYpe = 'Hourly' AND Active = 1 ORDER BY EmployeeFullName</CommandText>
      </Query>
      <Fields>
        <Field Name="EmployeeID">
          <DataField>EmployeeID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="EmployeeFullName">
          <DataField>EmployeeFullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>
 SELECT BranchId, BranchName FROM Branch ORDER BY BranchName</CommandText>
      </Query>
      <Fields>
