<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition"><Body><Height>2in</Height><Style /></Body><Width>6.5in</Width><Page><LeftMargin>1in</LeftMargin><RightMargin>1in</RightMargin><TopMargin>1in</TopMargin><BottomMargin>1in</BottomMargin><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="AceWOMDataSource"><DataSourceReference>AceWOMDataSource</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">2340358b-380c-4f9f-aa55-ecb6b1ea5a0a</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="DataSetSales"><Query><DataSourceName>AceWOMDataSource</DataSourceName><QueryParameters><QueryParameter Name="@BeginDate"><Value>=Parameters!BeginDate.Value</Value></QueryParameter><QueryParameter Name="@EndDate"><Value>=Parameters!EndDate.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT     Name AS Salesman, SUM(Sales) AS Sales, SUM(Cost) AS Cost, SUM(OrderCount) AS OrderCount
FROM         (SELECT     U.Name, SUM(T.MerchAmt) AS Sales, SUM(T.CostAmt) AS Cost, COUNT(*) AS OrderCount, E.Position
                       FROM          TSsoPastInvcHdr AS T INNER JOIN
                                              aspnet_Users AS U ON U.TakeStockLogin = T.Slsp INNER JOIN
                                              Employee AS E ON 'ACEWORLD\' + E.SQL_Login = U.UserName
                       WHERE      (T.OrderDate &gt;= @BeginDate) AND (T.OrderDate &lt;= @EndDate) AND (E.Position = 'Salesperson (Inside)')
                       GROUP BY T.Slsp, U.Name, E.Position
                       UNION
                       SELECT     U.Name, SUM(T.SOMerchAmt) AS Sales, SUM(T.SOCostAmt) AS Cost, COUNT(*) AS OrderCount, E.Position
                       FROM         TSsoDocHdr AS T INNER JOIN
                                             aspnet_Users AS U ON U.TakeStockLogin = T.Slsp INNER JOIN
                                             Employee AS E ON 'ACEWORLD\' + E.SQL_Login = U.UserName
                       WHERE     (T.OrderDate &gt;= @BeginDate) AND (T.OrderDate &lt;= @EndDate) AND (E.Position = 'Salesperson (Inside)') AND (T.DocType = 2)
                       GROUP BY T.Slsp, U.Name, E.Position) AS A
GROUP BY Name</CommandText></Query><Fields><Field Name="Salesman"><DataField>Salesman</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Sales"><DataField>Sales</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="Cost"><DataField>Cost</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="OrderCount"><DataField>OrderCount</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="BeginDate"><DataType>String</DataType><Prompt>BeginDate</Prompt></ReportParameter><ReportParameter Name="EndDate"><DataType>String</DataType><Prompt>EndDate</Prompt></ReportParameter></ReportParameters><Language>en-US</Language><ConsumeContainerWhitespace>true</ConsumeContainerWhitespace><rd:ReportUnitType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Inch</rd:ReportUnitType><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">58f2e3dd-bcd3-493a-8029-8c18031046a4</rd:ReportID></Report>