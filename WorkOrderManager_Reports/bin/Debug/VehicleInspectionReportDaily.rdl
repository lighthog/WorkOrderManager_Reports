up></TablixMember><TablixMember><Group Name="Details" /><SortExpressions><SortExpression><Value>=Fields!ReportExists.Value</Value><Direction>Descending</Direction></SortExpression><SortExpression><Value>=Fields!BranchID.Value</Value></SortExpression><SortExpression><Value>=Fields!EmployeeFullName.Value</Value></SortExpression></SortExpressions></TablixMember></TablixMembers></TablixRowHierarchy><DataSetName>DataSetVehicleInspection</DataSetName><Top>0.40292in</Top><Height>0.5in</Height><Width>6.5in</Width><Style><Border><Style>None</Style></Border></Style></Tablix><Textbox Name="Textbox13"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>="DVIR's for " &amp; FORMATDATETIME(Parameters!Date.Value)</Value><Style><FontSize>12pt</FontSize><FontWeight>Bold</FontWeight></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Textbox13</rd:DefaultName><Height>0.25in</Height><Width>6.5in</Width><ZIndex>1</ZIndex><Style><Border><Style>None</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></ReportItems><Height>0.90292in</Height><Style /></Body><Width>6.5in</Width><Page><LeftMargin>1in</LeftMargin><RightMargin>1in</RightMargin><TopMargin>1in</TopMargin><BottomMargin>1in</BottomMargin><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DataSource1"><DataSourceReference>AceWOMDataSource</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">20c47673-1d9b-4917-888a-27ca6d205f13</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="DataSetVehicleInspection"><Query><DataSourceName>DataSource1</DataSourceName><QueryParameters><QueryParameter Name="@Date"><Value>=Parameters!Date.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT        V.VehicleInspectionID, E.EmployeeFullName, E.BranchID, Vehicle.VehicleNumber, V.Odometer, V.Remarks + '      ' + V.TruckNo AS Remarks, V.TrailerNo, 
                         V.Satisfactory, E.EmployeeID, 'True' AS ReportExists
FROM            VehicleInspection AS V INNER JOIN
                         Employee AS E ON E.EmployeeID = V.EmployeeID INNER JOIN
                         Vehicle ON Vehicle.EmployeeID = V.EmployeeID
WHERE        (CAST(V.Date AS Date) = @Date)
UNION
SELECT DISTINCT 
                         NULL AS Expr1, E.EmployeeFullName, E.BranchID, Vehicle_1.VehicleNumber, NULL AS Expr5, NULL AS Expr6, NULL AS Expr7, NULL AS Expr8, E.EmployeeID, 
                         'False' AS Expr10
FROM            DispatchLine AS D INNER JOIN
                         Employee AS E ON E.EmployeeID = D.EmployeeID INNER JOIN
                         WorkOrder AS W ON W.WONum = D.WONum INNER JOIN
                         Vehicle AS Vehicle_1 ON Vehicle_1.EmployeeID = D.EmployeeID
WHERE        (CAST(D.EndTime AS Date) = @Date) AND (D.EmployeeID NOT IN
                             (SELECT        EmployeeID
                               FROM            VehicleInspection
                               WHERE        (CAST(Date AS Date) = @Date)))</CommandText></Query><Fields><Field Name="VehicleInspectionID"><DataField>VehicleInspectionID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="EmployeeFullName"><DataField>EmployeeFullName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="BranchID"><DataField>BranchID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="VehicleNumber"><DataField>VehicleNumber</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Odometer"><DataField>Odometer</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Remarks"><DataField>Remarks</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="TrailerNo"><DataField>TrailerNo</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Satisfactory"><DataField>Satisfactory</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Boolean</rd:TypeName></Field><Field Name="EmployeeID"><DataField>EmployeeID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="ReportExists"><DataField>ReportExists</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="DataSetDefaultDate"><Query