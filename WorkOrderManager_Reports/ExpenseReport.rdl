      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT        ExpenseItems.ExpenseID, ExpenseItems.EmployeeID, ExpenseItems.CustomerID, ExpenseItems.ExpenseDate, ExpenseItems.ExpenseCategory, 
                         ExpenseItems.Mileage, ExpenseItems.Amount, ExpenseItems.Description, ExpenseItems.DateLogged, ExpenseItems.LoggedBy, 
                         Employee.EmployeeFullName, Customer.CustomerName + ' - ' + Customer.AceID AS CustomerNameID
FROM            ExpenseItems INNER JOIN
                         Employee ON ExpenseItems.EmployeeID = Employee.EmployeeID LEFT OUTER JOIN
                         Customer ON ExpenseItems.CustomerID = Customer.CustomerID
WHERE        (ExpenseItems.ExpenseDate &gt;= @BeginDate) AND (ExpenseItems.ExpenseDate &lt;= @EndDate) AND (Employee.EmployeeID = @EmployeeID)</CommandText>
      </Query>
    </DataSet>
  </DataSets>
