    <DataSource Name="StorefrontDataSource">
      <DataSourceReference>WebsiteDataSource</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>c60ba4fa-75ae-4dac-b7b5-d36ad483906e</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="WeeklySales">
      <Query>
        <DataSourceName>StorefrontDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BeginDate">
            <Value>=Parameters!BeginDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        COUNT(*) AS OrderCount, SUM(OrderTotal) AS OrderTotals, DATEPART(yy, OrderDate) AS Year, DATEPART(mm, OrderDate) AS Month, DATEPART(wk, OrderDate) 
                         AS Week
FROM            Orders
WHERE        (OrderDate &gt;= @BeginDate) AND (OrderDate &lt;= @EndDate) AND (TransactionState = 'CAPTURED')
GROUP BY DATEPART(yy, OrderDate), DATEPART(mm, OrderDate), DATEPART(wk, OrderDate)</CommandText>
      </Query>
      <Fields>
        <Field Name="OrderCount">
          <DataField>OrderCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="OrderTotals">
          <DataField>OrderTotals</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Week">
          <DataField>Week</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
