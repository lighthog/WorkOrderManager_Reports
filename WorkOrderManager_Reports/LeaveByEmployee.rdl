    <DataSet Name="DataSetPayPeriodList">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT        PayPeriodID, CONVERT(varchar, StartDate, 101) + ' - ' + CONVERT(varchar, EndDate, 101) AS Period
FROM            PayPeriod
ORDER BY StartDate</CommandText>
      </Query>
      <Fields>
        <Field Name="PayPeriodID">
          <DataField>PayPeriodID</DataField>
          <rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName>
