        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSetGetLastPayPeriod">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT        PayPeriodID - 1 AS LastPayPeriod
FROM            PayPeriod
WHERE        (GETDATE() &gt;= StartDate) AND (GETDATE() &lt;= EndDate)</CommandText>
      </Query>
      <Fields>
        <Field Name="LastPayPeriod">
          <DataField>LastPayPeriod</DataField>
          <rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Employee">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT        EmployeeID, FirstName + ' ' + LastName AS EmpName, Active
FROM            Employee
WHERE        (Active = 'True')
ORDER BY EmpName</CommandText>
      </Query>
      <Fields>
        <Field Name="EmployeeID">
