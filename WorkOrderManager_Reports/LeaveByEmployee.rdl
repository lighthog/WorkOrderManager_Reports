  <DataSets>
    <DataSet Name="DataSetLeave">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EmployeeID">
            <Value>=Parameters!EmployeeID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        FirstName, LastName, HourDiff, BeginTime, Category, StartDate, EndDate, LoggedBy
FROM            (SELECT        Employee.FirstName, Employee.LastName, TimeLineItem.HourDiff, TimeLineItem.BeginTime, TimeLineItem.Category, PayPeriod.StartDate, 
                                                    PayPeriod.EndDate, TimeLineItem.LoggedBy
                          FROM            TimeLineItem INNER JOIN
                                                    Employee ON TimeLineItem.EmployeeID = Employee.EmployeeID INNER JOIN
                                                    PayPeriod ON TimeLineItem.BeginTime &gt;= PayPeriod.StartDate AND TimeLineItem.BeginTime &lt;= PayPeriod.EndDate
                          WHERE        (TimeLineItem.Category NOT IN ('Billable', 'Shop', 'Holiday')) AND (YEAR(PayPeriod.StartDate) = @Year) AND (Employee.EmployeeID IN (@EmployeeID))
                          UNION
                          SELECT        Employee_1.FirstName, Employee_1.LastName, EmployeeTimeSalary.Hours, EmployeeTimeSalary.Date, EmployeeTimeSalary.Category, 
                                                   PayPeriod_1.StartDate, PayPeriod_1.EndDate, EmployeeTimeSalary.LoggedBy
                          FROM            EmployeeTimeSalary INNER JOIN
                                                   Employee AS Employee_1 ON EmployeeTimeSalary.EmployeeID = Employee_1.EmployeeID INNER JOIN
                                                   PayPeriod AS PayPeriod_1 ON EmployeeTimeSalary.Date &gt;= PayPeriod_1.StartDate AND EmployeeTimeSalary.Date &lt;= PayPeriod_1.EndDate
                          WHERE        (EmployeeTimeSalary.Category NOT IN ('Billable', 'Shop', 'Holiday')) AND (YEAR(PayPeriod_1.StartDate) = @Year) AND 
                                                   (Employee_1.EmployeeID IN (@EmployeeID))) AS A
ORDER BY LastName, FirstName, BeginTime</CommandText>
      </Query>
