  <DataSets>
    <DataSet Name="DataSetExpenseItems">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ProfitabilityPeriodID">
            <Value>=Parameters!ProfitabilityPeriodID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Employee.EmployeeFullName, ExpenseItems.EmployeeID, ExpenseItems.ExpenseCategory, ExpenseItems.Amount, P.ProfitabilityPeriodID, 
                         ExpenseCategory.GLAccount, ExpenseCategory.Ordinal, Employee.GLDepartment AS Department, P.Description, ExpenseItems.GLCostCenter
FROM            ExpenseItems INNER JOIN
                         Employee ON ExpenseItems.EmployeeID = Employee.EmployeeID INNER JOIN
                         ProfitabilityPeriod AS P ON ExpenseItems.DateApproved &gt;= P.BeginDate AND ExpenseItems.DateApproved &lt;= P.EndDate INNER JOIN
                         ExpenseCategory ON ExpenseItems.ExpenseCategory = ExpenseCategory.ExpenseCategory
WHERE        (P.ProfitabilityPeriodID = @ProfitabilityPeriodID) AND (ExpenseItems.Approved = 'True') AND (ExpenseItems.ExceptionApproved = 'False')</CommandText>
      </Query>
