    <DataSet Name="dsBranches">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT BranchID, BranchName 
     FROM Branch 
     Where BranchName != 'All'
     ORDER BY BranchName</CommandText>
      </Query>
