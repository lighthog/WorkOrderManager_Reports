  <DataSets>
    <DataSet Name="dsMainQuery">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@rpBranch">
            <Value>=Parameters!rpBranch.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@rpComplete">
            <Value>=Parameters!rpComplete.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="rpFromVal">
            <Value>=Int(Split(Parameters!rpDueIn.Value,",")(0))</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="rpToVal">
            <Value>=Int(Split(Parameters!rpDueIn.Value,",")(1))</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- This part only gets those tasks linked to a work order
SELECT TOP 200 WOT.WorkOrderTaskId, B.BranchName,  WOT.DueDate, EMP.EmployeeFullName, WOT.Complete, 
                            WOT.DateCompleted, WOT.CompletedBy, WO.WONum,  dbo.Ellipses(WOT.Description, 120) AS Description, 
                            WO.CustomerID, C.CustomerName
	FROM WorkOrderTask WOT
	INNER JOIN Employee EMP ON EMP.EmployeeID = WOT.AssignedTo
	INNER JOIN WorkOrder WO ON WO.WONum = WOT.WONum 
	INNER JOIN Branch B ON B.BranchID = WO.BranchID 
	LEFT OUTER JOIN Customer C ON C.CustomerID = WO.CustomerID 
	WHERE B.BranchID IN (@rpBranch)
	  AND WOT.Complete IN (@rpComplete)
	  AND DueDate BETWEEN DateAdd(d, @rpFromVal, GetDate()) AND DateAdd(d, @rpToVal, GetDate()) 

UNION    -- This part gets those not connected to a work order

SELECT TOP 200 WOT.WorkOrderTaskId, B.BranchName,  WOT.DueDate, EMP.EmployeeFullName, WOT.Complete, 
                            WOT.DateCompleted, WOT.CompletedBy, WOT.WONum,  dbo.Ellipses(WOT.Description, 120) AS Description, 
                            NULL AS CustomerID, '' AS CustomerName
	FROM WorkOrderTask WOT
	INNER JOIN Employee EMP ON EMP.EmployeeID = WOT.AssignedTo
	LEFT OUTER JOIN Branch B ON B.BranchID = EMP.BranchID 
	WHERE WOT.WONum IS NULL
	  AND B.BranchID IN (@rpBranch)
	  AND WOT.Complete IN (@rpComplete)
	  AND DueDate BETWEEN DateAdd(d, @rpFromVal, GetDate()) AND DateAdd(d, @rpToVal, GetDate()) 
      ORDER BY DueDate ASC </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
