  <DataSets>
    <DataSet Name="DataSetBranchSales">
      <Fields>
        <Field Name="MerchAmt">
          <DataField>MerchAmt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Cost">
          <DataField>Cost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Whse">
          <DataField>Whse</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT     SUM(I.MerchAmt) AS MerchAmt, P.Description, SUM(I.CostAmt) AS Cost, I.Whse
FROM         TSsoPastInvcHdr AS I RIGHT OUTER JOIN
                      ProfitabilityPeriod AS P ON I.InvcDate &gt;= P.BeginDate AND I.InvcDate &lt;= P.EndDate
WHERE     (I.Whse = @Whse)
GROUP BY P.Description, P.ProfitabilityPeriodID, I.Whse
HAVING      (P.ProfitabilityPeriodID &gt;= @Period - 12)
ORDER BY P.ProfitabilityPeriodID</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Whse">
            <Value>=Parameters!Whse.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Period">
            <Value>=Parameters!Period.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
  </DataSets>
